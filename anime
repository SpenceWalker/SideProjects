DROP TABLE IF EXISTS manga, animes, anime_manga, anime_movies, anime_series_movies, anime_characters;

CREATE TABLE manga
(
	manga_id SERIAL PRIMARY KEY,
	title VARCHAR(250),
	author_name VARCHAR (100) NOT NULL,
	release_date DATE NOT NULL,
	is_finished BOOLEAN,
	is_animated BOOLEAN
	
);

CREATE TABLE animes
(
	anime_id SERIAL PRIMARY KEY,
	title VARCHAR(500)NOT NULL,
	release_date DATE NOT NULL,
	studio_publisher VARCHAR(500) NOT NULL,
	number_of_episodes INTEGER, 
	still_being_released BOOLEAN,
	genre VARCHAR NOT NULL,
	overview TEXT
	
);

CREATE TABLE anime_manga
(
	manga_id INTEGER,
	anime_id INTEGER,
	
	CONSTRAINT fk_manga_id FOREIGN KEY(manga_id) REFERENCES manga (manga_id),
	CONSTRAINT fk_anime_id FOREIGN KEY(anime_id) REFERENCES animes (anime_id)
);


CREATE TABLE anime_movies
(
	movie_id SERIAL PRIMARY KEY, 
	title VARCHAR(500),
	length_minutes INTEGER,
	release_date DATE,
	genre VARCHAR(100),
 	is_cannon BOOLEAN NOT NULL

);

CREATE TABLE anime_series_movies
(
	movie_id INTEGER,
	anime_id INTEGER,
	
	CONSTRAINT fk_movie_id FOREIGN KEY (movie_id) REFERENCES anime_movies (movie_id),
	CONSTRAINT fk_anime_id FOREIGN KEY (anime_id) REFERENCES animes (anime_id)
);


CREATE TABLE anime_characters
(
	character_id SERIAL PRIMARY KEY, 
	anime_id INTEGER,
	first_name VARCHAR (50),
	last_name VARCHAR (50),
	birth_month INTEGER,
	birth_date INTEGER,
	is_dead BOOLEAN,
	age INTEGER NOT NULL,
	home_planet VARCHAR(100)NOT NULL,
	occupation VARCHAR(300),
	good BOOLEAN,
	specialties_or_quirks VARCHAR(500),
	personality_traits VARCHAR (1500),
	overview TEXT,
	is_main_character BOOLEAN,
	
	CONSTRAINT fk_anime_id FOREIGN KEY (anime_id) REFERENCES animes (anime_id)
	
);




